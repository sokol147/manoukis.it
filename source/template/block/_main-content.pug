- var mainNewsImg = 'main-news.png'
- var mainNewsTag = 'Technika'
- var mainNewsHeader = 'Briuselis žada sąžiningesnes išmokas žemdirbiams'
- var mainNewsDescr = 'Lapkričio 30d., šeštadienis  Mokslas'
- var mainNewsText = 'Vilnius. Briuselis ieškos būdų, kaip sąžiningiau paskirstyti tiesiogines išmokas žemdirbiams skirtingose Europos Sąjungos (ES) šalyse, tačiau konkrečių skaičių naujajame biudžete po 2020 metų kol kas nekomentuoja, sako už žemės ūkį atsakingas eurokomisaras Filas Hoganas (Phil Hogan). '

.main-content
	.container
		.row
			.col-lg-2.left-banner-column
				.side-menu
					
				.banner-block-l
					ul.left-banner
						each lBannerImg,lBannerLink in leftBanners
							li.left-banner__item
								a.left-banner__link(href=`${lBannerLink}`)
									img.left-banner__img(src=`templates/images/lBanners/${lBannerImg}`)

			.col-lg-7.main-news

				.m-banner
					a.m-banner__link(href=`${mBannerLink}`)
						img.m-banner__img(src=`templates/images/mBanners/${mBannerImg}`)

				.news-block
						.news-block-big
							a.news__img-link(href=`${mainNewsLink}`)
								img.main__img(src=`templates/images/newsList/${mainNewsImg}`)
							.main__content
								p.main__tag= mainNewsTag
								a.news__link(href=`${mainNewsLink}`)
									p.main__header= mainNewsHeader
								p.main__description= mainNewsDescr
								p.main__text= mainNewsText
									a.news__read-more(href=`${mainNewsLink}`) Daugiau
						ul.news-block-small
							each item in newsBlock
								li.news-block-element
									a.news__link(href=`${item.newsBlockLink}`)
										img.news-block__img(src=`templates/images/newsList/${item.newsBlockImg}`)
										p.news__header= item.newsBlockHeader
									p.news__description= item.newsBlockDescr
									p.news__text= item.newsBlockText
										a.news__read-more(href=`${item.newsBlockLink}`) Daugiau


				.g-banner
					a.g-banner__link(href=`${gBannerLink}`)
						img.g-banner__img(src=`templates/images/mBanners/${gBannerImg}`)

				.galleries
					.galleries__header=galleriesHeader
					ul.gallery__list
						each item in gallery
							li.gallery__item
								img.gallery__img(src=`templates/images/gallery/${item.img}`)
								.gallery__descr
									a.gallery__link(href=`${item.link}`)=item.name
									p.gallery__counter=item.counter

				ul.news__list
					each item in newsList
						li.news__item
							a.news__img-link(href=`${item.newsListLink}`)
								img.news__img(src=`templates/images/newsList/${item.newsListImg}`)
							.news__content
								a.news__header(href=`${item.newsListLink}`)= item.newsListHeader
								p.news__description= item.newsListDescr
								p.news__text= item.newsListText
									a.news__read-more(href=`${item.newsListLink}`) Daugiau

			.col-lg-3.right-banner-column
				.right-banner.right-banner-top
					a.right-banner__link(href=`${rBannerTopLink}`)
						img.right-banner__img(src=`templates/images/rBanners/${rNammerTopImg}`)

				form.subscription
					p.subscription__text=subText
					input.subscription__input(type='text' placeholder='Jūsų el. pašto adresas')
					a.btn-sub=subBtnText
				.right-banner.right-banner-group
					ul.right-banner
						each rBannerImg,rBannerLink in rightBanners
							li.right-banner__item
								a.right-banner__link(href=`${rBannerLink}`)
									img.right-banner__img(src=`templates/images/rBanners/${rBannerImg}`)
				.appearance.widget
					.appearance__header.widget__header=appearanceHeader
					form.appearance__question-wrapper
						.appearance__question=appearanceQuestion
						ul.appearance__answer.answer
							each item in appearanceAnswer
								li.answer__item
									label.answer__label.label--radio=item
										input.answer__input.radio(type='radio' name='question')
						a.btn-appearance=appearenceBtnText
				.weather.widget
					.widget__header=weatherHeader
				.business-info.widget
					.business-info__header.widget__header.widget__header-green=businessInfoHeader
					ul.business-news
						each businesNewsName, businessNewsLink in businessNews
							li.business-news__item
								a.business-news__link(href=`${businessNewsLink}`)=businesNewsName
				.advertising.widget
					.advertising__header.widget__header=advertisingHeader
					ul.advertising__list
						each advertisingName, advertisingLink in advertising
							li.advertising__item
								a.advertising__link(href=`${advertisingLink}`)=advertisingName
