.main-content
	.container
		.row
			.col-lg-2.left-banner-column
				ul.side-menu
					li.side-menu__item
						a.side-menu__link.side-menu__link-active(href='#') Naujienos
						ul.side-menu__sub
							each subItem, subLink in subMenu
								li.sub__item
									a.sub__link(href=`${subLink}`)= subItem
					li.side-menu__item
						a.side-menu__link(href='#') MANO ŪKIS, žurnalas
					li.side-menu__item
						a.side-menu__link(href='#') Spaudos centras
				select.select-mobile-only
					option Naujienos
					option Agropolitika
					option Pasaulyje
					option Ūkis
					option Rinka
					option MANO ŪKIS, žurnalas
					option Spaudos centras

				.banner-block-l
					ul.left-banner
						each lBannerImg,lBannerLink in leftBanners
							li.left-banner__item
								a.left-banner__link(href=`${lBannerLink}`)
									img.left-banner__img(src=`templates/images/lBanners/${lBannerImg}`)

			.col-lg-7.main-news

				.m-banner
					a.m-banner__link(href=`${mBannerLink}`)
						img.m-banner__img(src=`templates/images/mBanners/${mBannerImg}`)
				.news-content
					.news__topic= newsTopic
					.news__header= newsHeader
					.news__date= newsDate
					.news__text
						figure.figure-in-text
							img.figure-img(src=`templates/images/news/${newsImgFirst}`)
							figcaption.figure-descr= newsImgFirstDescr
						p= newsOneP
						p= newsTwoP
						p= newsThreeP
						p= newsFourP
						p= newsFiveP
						p= newsSixP
						figure.figure-in-text
							img.figure-img(src=`templates/images/news/${newsImgSecond}`)
							figcaption.figure-descr= newsImgSecondDescr
						p= newsSevenP
						p= newsEightP
						p= newsNineP
						p BNS, ZUM, MU inf.
					.news__gallery
						.gallery-header= galleryHeader
						ul.gallery__list
							each galleryImg, galleryLink in galleryList
								li.gallery__item
									a.gallery__link(href=`${galleryLink}`)
										img.gallery__img(src=`templates/images/news-gallery/${galleryImg}`)
					.news__share
						a(href='#').news__print= newsPrint
						.news__counter
						//- ul.news__soical
						//- 	each item in newsSocial
						//- 		li.share__item
						//- 			a.share__link(href=``)
					form.form-upload
						p.upload__header= uploadHeader
						p.upload__descr= uploadDescr
						.upload__confirmation
							p.confirm__message= confirmMessage
								span.confirm__word= confirmWord
							input.form-field
						.upload__file-select
							p.select__header= fileSelectHeader
							label.select__label= fileSelectBtn
								input.select__input(type='file' size='20')
				.g-banner
					a.g-banner__link(href=`${gBannerLink}`)
						img.g-banner__img(src=`templates/images/mBanners/${gBannerImg}`)
				.comments
					p.comments__counter= commentsCounter
					form.comments__form
						p.comments__descr= commentMessage
							span= commentMessageSpan
						p.comments__descr= commentMessageTwo
						.comments__inputs
							input.form-field(type='text' placeholder='Jūsų vardas')
							input.form-field(type='text' placeholder='Jūsų el. paštas')
						textarea.form-textarea
						.upload__confirmation
							p.confirm__message= confirmMessage
								span.confirm__word= confirmWord
							input.form-field
							a(href='#').comments__btn= commentsBtn
					ul.comment__list
						each item in commnets
							li.comment__item
								img.commentator__img(src=`templates/images/comment/${item.userImg}`)
								.comment__wrapper
									p.commentator__name= item.userName
									p.comment__date= item.commentDate
									p.commentator__text= item.commentText
					a.comment__load-more(href='#')= btnLoadMore
				.also-read
					p.also-read__header Taip pat skaitykite
					.also-read__news
						ul.news__list
							each item in newsList
								li.news__item
									a.news__img-link(href=`${item.newsListLink}`)
										img.news__img(src=`templates/images/newsList/${item.newsListImg}`)
									.news__content
										a.news__header(href=`${item.newsListLink}`)= item.newsListHeader
										p.news__description.news-with-photo= item.newsListDescr
										p.news__comment= item.newsListComment
										p.news__text= item.newsListText
											a.news__read-more(href=`${item.newsListLink}`) Daugiau
					ul.also-read__list
						each alsoReadText,alsoReadLink in alsoRead
							li.also-read__item
								a.also-read__link(href=`${alsoReadLink}`)=alsoReadText

			.col-lg-3.right-banner-column
				.right-banner.right-banner-top
					a.right-banner__link(href=`${rBannerTopLink}`)
						img.right-banner__img(src=`templates/images/rBanners/${rNammerTopImg}`)

				form.subscription
					p.subscription__text=subText
					input.subscription__input(type='text' placeholder='Jūsų el. pašto adresas')
					a.btn-sub=subBtnText
				.right-banner.right-banner-group
					ul.right-banner
						each rBannerImg,rBannerLink in rightBanners
							li.right-banner__item
								a.right-banner__link(href=`${rBannerLink}`)
									img.right-banner__img(src=`templates/images/rBanners/${rBannerImg}`)
				.appearance.widget
					.appearance__header.widget__header=appearanceHeader
					form.appearance__question-wrapper
						.appearance__question=appearanceQuestion
						ul.appearance__answer.answer
							each item in appearanceAnswer
								li.answer__item
									label.answer__label.label--radio=item
										input.answer__input.radio(type='radio' name='question')
						a.btn-appearance=appearenceBtnText
				.weather.widget
					.widget__header=weatherHeader
				.business-info.widget
					.business-info__header.widget__header.widget__header-green=businessInfoHeader
					ul.business-news
						each businesNewsName, businessNewsLink in businessNews
							li.business-news__item
								a.business-news__link(href=`${businessNewsLink}`)=businesNewsName
				.advertising.widget
					.advertising__header.widget__header=advertisingHeader
					ul.advertising__list
						each advertisingName, advertisingLink in advertising
							li.advertising__item
								a.advertising__link(href=`${advertisingLink}`)=advertisingName